{
	"name": "devnet-directory",
	"version": "0.0.1",
	"private": true,
	  "packageManager": "pnpm@8.15.6",
	"engines": {
		"node": ">=18",
		"pnpm": ">=8"
	},
	"scripts": {
		"start:dev": "dotenv -- vite dev --port 5176",
		"dev": "pnpm run start:dev",
		"build": "dotenv -- vite build",
		"download": "tsx src/scripts/download.ts",
		"format-directory": "tsx src/scripts/format.ts",
		"docker:build": "docker build -t devnet-directory .",
		"db:fake": "tsx db/test/generate-fake.ts",
		"db:push": "prisma db push",
		"start": "dotenv -- node ./build/index.js",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-node": "1.0.0-next.106",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "^8.56.0",
		"@types/node": "^20",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"autoprefixer": "^10.4.19",
		"daisyui": "^4.10.2",
		"dotenv-cli": "^7.4.2",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0-next.4",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.18",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.4.3",
		"tslib": "^2.4.1",
		"tsx": "^4.10.0",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"type": "module",
	"dependencies": {
		"@lucia-auth/adapter-prisma": "^4.0.1",
		"@prisma/client": "^5.13.0",
		"@solana/web3.js": "^1.93.0",
		"@tanstack/svelte-query": "^5.32.0",
		"@tanstack/svelte-table": "^8.17.3",
		"@trpc/client": "^10.45.2",
		"@trpc/server": "^10.45.2",
		"arctic": "^1.5.0",
		"bits-ui": "^0.21.10",
		"clsx": "^2.1.1",
		"data": "workspace:*",
		"lucia": "^3.2.0",
		"lucide-svelte": "^0.376.1",
		"mode-watcher": "^0.3.0",
		"oslo": "^1.2.0",
		"prisma-shared": "workspace: *",
		"svelte-intersection-observer": "^1.0.0",
		"tailwind-merge": "^2.3.0",
		"tailwind-variants": "^0.2.1",
		"trpc-svelte-query": "^2.1.0",
		"types": "workspace: *",
		"zod": "^3.23.4"
	}
}
